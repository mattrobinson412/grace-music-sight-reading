{
    "$id": "http://example.com/contactUpdate.json",
    "$schema": "http://json-schema.org/draft-07/schema",
    "default": {},
    "description": "The root schema comprises the entire JSON document.",
    "examples": [
        {
            "email": "garbage@gmail.com",
            "isHandled": true
        }
    ],
    "required": [
        "email", 
        "isHandled"
    ],
    "title": "The root schema",
    "type": "object",
    "properties": {
        "email": {
            "$id": "#/properties/email",
            "type": "string",
            "title": "The email schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "garbage@gmail.com"
            ]
        },
        "isHandled": {
            "$id": "#/properties/isHandled",
            "type": "boolean",
            "title": "The isHandled schema",
            "description": "An explanation about the purpose of this instance.",
            "default": false,
            "examples": [
                true
            ]
        }
    },
    "additionalProperties": true
}